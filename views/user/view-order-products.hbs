<section id="featured" class="section-p1">
    <div class="container text-center mt-5 py-5">
        <h3>Ordered Products</h3>
        <hr>
    </div>
    <div class="pro-container">
        {{#each products}}
      <div class="pro">
          <img src="/product-images/{{this.product._id}}1.jpg" alt="" onclick="window.location.href='/view-product/{{this.product._id}}'">
